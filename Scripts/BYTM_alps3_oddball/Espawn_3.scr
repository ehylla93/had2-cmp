SendSignal(es5, 1);
SendSignal(p01, 1);
SendSignal(p02, 1);
SendSignal(E01, 1);
SendSignal(E02, 1);
SendSignal(E03, 1);
SendSignal(E04, 1);
SendSignal(E05, 1);
SendSignal(E06, 1);
SendSignal(E07, 1);
SendSignal(E08, 1);
SendSignal(E09, 1);
SendSignal(E10, 1);
SendSignal(E11, 1);
SendSignal(E12, 1);
}
EndScript();
}

OnSignal(1)
{
EnableSignal(1, 0);
printf("Espawn3 Whenever actice");
SetWhenever(PIB, 1);
}

