// RADIO1 - RADIO MISSIO
// ----------------------------------------


Block
{
Frame me;	FRM_GetMyFrame(me);
Frame objectives;	FRM_FindFrame(objectives, "objectives");
}



// ----------------------------------------

OnDeath(){
	SUBTITLES_SetOn(1);
	SUBTITLES_SetText(59999601);	//"OBJECTIVE ACCOMPLISHED: RADIO DESTROY."
	Delay(5000);
	SUBTITLES_SetOn(0);
	SetObjectiveStatus(8, 1);
	GoTo end;
}


Label END: