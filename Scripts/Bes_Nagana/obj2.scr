// gold box


Block
{
Frame objectives;	FRM_FindFrame(objectives, "objectives");
}


Whenever player(_PlayerInRange(2))
{
	SendSignal(objectives, 10);
	SUBTITLES_SetOn(true);
	SUBTITLES_SetText(21990030);		//"OPTIONAL OBJECTIVE ACCOMPLISHED: STOLEN VALUABLES FOUND."
	Delay(6000);
	SUBTITLES_SetOn(false);
}
GoTo end;



Label END:

