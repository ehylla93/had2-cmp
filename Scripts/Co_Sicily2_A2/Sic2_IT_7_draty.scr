OnAlarm()
{
  Block
    {
      DisableSignals(true);
      DisableWhenevers(true);
      AIEvent =_GetAlarmType(); 
      PrintfNumber(" IT 7: ", AIEvent);	
      HUMAN_Stop();
      HUMAN_SETMODE_Run();
    }
  EndScript();
}

