// Spawnzone 2

block {
FRAME me;      		FRM_GetMyFrame(me);

MP_EnableSpawnZone("zone2", 2);
}

Whenever EnSpawn2 ((_PlayerInRange(50)) AND (not _EnemyInRange(me, 100)))
{
SetWhenever(EnSpawn2, 0);
MP_EnableSpawnZone("zone2", 1);
SUBTITLES_SetOn(true);
SUBTITLES_SetText(59995027);
Delay(3000);
SUBTITLES_SetOn(false);
EndScript();
}