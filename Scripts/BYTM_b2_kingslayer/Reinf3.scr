// Reinf3

Block
{
Frame E09;	FRM_FindFrame(E09,"C3_E01");
Frame E10;	FRM_FindFrame(E10,"C3_E02");
Frame E11;	FRM_FindFrame(E11,"C3_E03");
Frame E12;	FRM_FindFrame(E12,"C3_E04");
Frame E13;	FRM_FindFrame(E13,"C3_E05");
Frame E14;	FRM_FindFrame(E14,"C3_E06");

Frame R3_E01;	FRM_FindFrame(R3_E01,"R3_E01");
Frame R3_E02;	FRM_FindFrame(R3_E02,"R3_E02");
