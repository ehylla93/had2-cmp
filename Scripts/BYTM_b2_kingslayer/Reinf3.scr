}

// ----------------------------------------

Whenever Reinforcements3 ((((((_Actor_GetState(E09) == 0) AND (_Actor_GetState(E10) == 0)) AND 
(_Actor_GetState(E11) == 0)) AND (_Actor_GetState(E12) == 0)) AND (_Actor_GetState(E13) == 0)) AND 
(_Actor_GetState(E14) == 0))
{
printf("Reinf3 countdown started");
Timer = _RandomInt(5);
	If (Timer == 0)
	{
	Delay(5000);
	}
	
	If (Timer == 1)
	{
	Delay(10000);
	}
	
	If (Timer == 2)
	{
	Delay(15000);
	}
	
	If (Timer == 3)
	{
	Delay(20000);
	}
	
	If (Timer == 4)
	{
	Delay(25000);
	}

SendSignal(text, 1);
Block { SendSignal(R3_E01, 1);SendSignal(R3_E02, 1);SendSignal(R3_E03, 1);SendSignal(R3_E04, 1);SendSignal(R3_E05, 1);SendSignal(R3_E06, 1); }
