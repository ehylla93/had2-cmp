// Spawnzone3

Block {
Frame me;	FRM_GetMyFrame(me);

MP_EnableSpawnZone("zone3", 2);
}

Whenever Active ((_PlayerInRange(15)) AND (!_EnemyInRange(me, 35)))
{
SetWhenever(Active, false);
MP_EnableSpawnZone("zone3", 1);
}

OnSignal(1)	//received from objassemble
{
SetWhenever(Active, false);
MP_EnableSpawnZone("zone3", 2);
}