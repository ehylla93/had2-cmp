// C5_G38_zbran - muzzle flash of pistol on table - off by default

Block
{
Frame me;		FRM_GetMyFrame(me);
Frame zasleh;	FRM_FindFrame(zasleh, "G38_zbran.flash");
}

Whenever signal(_SignalReceived(1))	// received from C5_G38
{
	FRM_SetOn(me, false);
	GoTo end;
}

FRM_SetOn(zasleh, false);

Label end: