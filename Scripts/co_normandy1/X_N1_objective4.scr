// objective_gather - objective gather troops at exit point
// -------------------------------------------------------------------------------------------------------------
// Variables definition
Block
{
Frame obj;
	FRM_FindFrame(obj, "objectives");
}
// -------------------------------------------------------------------------------------------------------------
// Whenever keywords and signals
Whenever players(_MP_AllTeamMembersAreInRange(1, 18))
{
	SendSignal(obj, 6);
	Delay(5000);
	SetWhenever(players, true);
}
// -------------------------------------------------------------------------------------------------------------