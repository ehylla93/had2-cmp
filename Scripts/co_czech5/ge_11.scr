OnAlarm()
	{
	Printf("Driver alarmed");
	EndScript();
	}

OnAlarmDone()
	{
	Printf("Driver alarm done");
	}
	
OnDeath()
{
SendSignal(count, 1);
EndScript();
}

Whenever AlarmsOff (_SignalReceived(20))
{
DisableAlarms(1);
}

