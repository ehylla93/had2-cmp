SetAlarmType(1023, false);
HUMAN_BoardVehicle("La_TigerEv_1", 1, 0);

OnSignal(1)
	{
	Label xx:
	HUMAN_Drive("xsec", 45);
	HUMAN_Drive("A21", 45);
	HUMAN_Drive("A22", 45);
