	HUMAN_Drive("A24", 45);
	HUMAN_Drive("A25", 45);

	HUMAN_Drive("A26", 40);
	HUMAN_Drive("A27", 40);
	HUMAN_Drive("A28", 40);
	HUMAN_Drive("A29", 40);
	HUMAN_Drive("A30", 40);
	HUMAN_Drive("A31", 40);
	HUMAN_Drive("A32", 40);
	HUMAN_Drive("A33", 40);
	HUMAN_Drive("A34", 40);
	HUMAN_Drive("A35", 40);
	HUMAN_Drive("A36", 40);
	HUMAN_Drive("A37", 40);
	HUMAN_Drive("B67", 40);
	HUMAN_Drive("B68", 40);
	HUMAN_Drive("A38", 40);
	HUMAN_Drive("A39", 40);
	HUMAN_Drive("A40", 40);
	HUMAN_Drive("A41", 40);
