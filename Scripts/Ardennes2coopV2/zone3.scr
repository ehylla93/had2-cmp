//Spawn3 (2. from south)
Block
{
Frame detect;	FRM_FindFrame (detect,"zone3");
Frame txt;		FRM_FindFrame(txt,"Text");
Frame Tiger_7;	FRM_FindFrame(Tiger_7,"Tiger_7");
}
goto end;



Whenever Yes ((_PlayerInRange(40)) and (_Actor_GetState(Tiger_7)==0) and (!_EnemyInRange(detect,50)))
{
Delay(5000);
MP_EnableSpawnZone("zone3", 1);
SendSignal(txt, 3);
goto end;
}

label end:
