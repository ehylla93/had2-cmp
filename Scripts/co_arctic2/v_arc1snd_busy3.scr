
Block {
frame sound1;		frm_findframe(sound1,"S_pip5");
frame sound2;		frm_findframe(sound2,"S_pip6");
frame sound3;		frm_findframe(sound3,"S_pip7");

integer rnd;	
integer rnd_pause;
}

label loop:
rnd= _randomint(2) +1;
rnd_pause= _randomint(8300) +9000;
delay(rnd_pause);
if (rnd==1)
{
	FRM_SetOn(sound1,true);
}
if (rnd==2)
{
	FRM_SetOn(sound2,true);
	delay(1400);
	FRM_SetOn(sound3, true);
}

goto loop;
