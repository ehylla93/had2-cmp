//Photo Counter

Block {
frame text;		FRM_FindFrame(text, "text");
frame OBJ3;		FRM_FindFrame(OBJ3, "dummy_KRVEPROLITI");
Integer counter = 0;
}

OnSignal(1)		// Photograph related - OBJ1
{
counter = counter + 1;
If ( counter == 3)
{
Delay(5000);
SendSignal(text, 6);
Delay(5000);
SendSignal(OBJ3, 7);
EndScript();
}
Else {goto END;}
}

Label END: