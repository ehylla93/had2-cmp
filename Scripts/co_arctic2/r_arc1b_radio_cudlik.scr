
Block {
frame Radio_sound_1;	 FRM_FindFrame(Radio_sound_1, "S_Radio");
integer hraje = 1;
FRM_SetOn(Radio_sound_1, True);
}

OnUse()
{
  If (hraje == 1)
    {
        FRM_SetOn(Radio_sound_1, false);
        hraje = 0; 
      }else{
        FRM_SetOn(Radio_sound_1, True);
        hraje = 1;
    }
}

OnSignal(1)
{
  Endscript();
}