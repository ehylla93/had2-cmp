// BU1_diary - diary
// -------------------------------------------------------------------------------------------------------------
// Variables definition
Integer players;
	players=0;
// -------------------------------------------------------------------------------------------------------------
// Whenever keywords and signals
OnSignal(1)	// received from bridge BU1_obj_04
{
	If(players==1)
	{
		AddDiaryText(32);
		AddDiaryText(4213);
		GoTo end;
	}
	AddDiaryText(32);
	AddDiaryText(4212);
	GoTo end;
}


// -------------------------------------------------------------------------------------------------------------
Delay(1000);
players=_GetNumMembersOfClientTeam();

Label end: