SetObjectiveStatus(4, 0);
SetWhenever(Players81, 0);

goto END;

// ----------------------------------

Whenever Players81 (_MP_AllTeamMembersAreInRange(1,8))
{

SendSignal(Spawnzones, 2);
SendSignal(Detector_05, 10);
SendSignal(Object324, 10);
SendSignal(Detector_32, 10);
SendSignal(Detector_33, 10);
SetObjectiveStatus(5, 0);
SUBTITLES_SetOn(1);
SUBTITLES_SetText(18993608);		//"OBJECTIVE COMPLETED: SQUAD ASSEMBLED IN THE EXTRACTION ZONE."
Delay(4000);
SUBTITLES_SetOn(0);
SetObjectiveStatus(4, 1);
Delay(1000);
SUBTITLES_SetOn(1);
SUBTITLES_SetText(17993404); // NEW OBJECTIVE: WAIT FOR REINFORCEMENTS.
Delay(5000);
SUBTITLES_SetOn(0);

Delay(1000);
SendSignal(Charon_2, 1);

SendSignal(Charon_1, 1);

SendSignal(Allied, 1);

  Delay(1000);
 SendSignal(Charon_2, 2);
 
   Delay(10000);
 SendSignal(Charon_1, 2);

  Delay(15000);
 SendSignal(Allied, 2);
 
  Delay(90000); 
SendSignal(renfor1,1);
SendSignal(renfor2,1);
SendSignal(renfor3,1);
SendSignal(renfor4,1);
SendSignal(renfor5,1);  
SendSignal(Snipe1,1);
SendSignal(Snipe2,1);
SendSignal(Snipe3,1);
SendSignal(Snipe4,1);
SendSignal(Snipe5,1);
SendSignal(Snipe7,1);
 

EndScript();
}

OnSignal(2)
{
SetWhenever(Players81, 1);
goto END;
}



