// Obj 5 - Assemble at the extraction zone
// ------------------------------------

Block
{
Frame renfor1;		FRM_FindFrame(renfor1,"renfor1");
Frame renfor2;		FRM_FindFrame(renfor2,"renfor2");
Frame renfor3;		FRM_FindFrame(renfor3,"renfor3");
Frame renfor4;		FRM_FindFrame(renfor4,"renfor4");
Frame renfor5;		FRM_FindFrame(renfor5,"renfor5");
Frame Snipe1;		FRM_FindFrame(Snipe1,"Snipe1");
Frame Snipe2;		FRM_FindFrame(Snipe2,"Snipe2");
Frame Snipe3;		FRM_FindFrame(Snipe3,"Snipe3");
Frame Snipe4;		FRM_FindFrame(Snipe4,"Snipe4");
Frame Snipe5;		FRM_FindFrame(Snipe5,"Snipe5");
Frame Snipe7;		FRM_FindFrame(Snipe7,"Snipe7");
Frame Spawnzones;		FRM_FindFrame(Spawnzones,"Spawnzones");
Frame Detector_33;	FRM_FindFrame(Detector_33,"Detector_33");
Frame Detector_32;	FRM_FindFrame(Detector_32,"Detector_32");
Frame Detector_05;	FRM_FindFrame(Detector_05,"Detector_05");
Frame Object324;	FRM_FindFrame(Object324,"Object324");
Frame me;	FRM_GetMyFrame(me);
frame Allied;   FRM_FindFrame(Allied,"Allied");
