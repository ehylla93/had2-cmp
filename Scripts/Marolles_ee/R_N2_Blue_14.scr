  integer counter = 0;


SetAlarmType(920, False);	// 8+16+128+256+512

HUMAN_Suspend(True);				
goto TheEnd;				

Onsignal(10)         
{
  HUMAN_Suspend(False);
  Printf("BLUE wake up ------ 14");
}


label DeAlarm:
  Block
    {
       HUMAN_WeaponOnArm(True);
       HUMAN_SETAIMODE_Defensive();
       HUMAN_SETMODE_Crouch();
       HUMAN_SetOptimized(2);
       HUMAN_SETMODE_Run();
    }
  HUMAN_Move("Red35_1");		// !
  HUMAN_Move("Go2_3a");			// !
  HUMAN_TurnAt(turn);	
  SetAlarmType(367, True);	// 16+128+512  = 656 False
goto TheEnd;				

OnSignal(1)	/////// Doplnit poslani od odesilatele (od Ally1)
{
  Label loop_A1:
    If (( _ACTOR_GetState(Ally1) == 0 ))    { Goto TheEnd; }
    If (counter == 6) { Goto TheEnd; }
    counter = counter +1;
    HUMAN_TurnAt(Ally1);	
    HUMAN_Attack(Ally1, 1000);
    Delay(1500);
  goto loop_A1;
}
