//Detector for Spawnzone 2

block
{
frame spawn;		FRM_FindFrame(spawn,"spawnzones");

SetWhenever(c, false);
}

Whenever active(_SignalReceived(1))
{
SetWhenever(active, false);
SetWhenever(c, true);
}

Whenever c(_PlayerInRange(20))
{
SetWhenever(c, false);
SendSignal (spawn,2);
}

label end: