
Block {
Frame ge01;		Frm_FindFrame (ge01,"ge_01");
integer time;
}

goto end;

Whenever PIR (_MP_IsTeamMemberInRange(1, 1000))
{
SetWhenever(PIR, false);
goto piskej;
}

label piskej:
FRM_MorphSpeech(ge01,07021400,0,20);
time = 15000 + _RandomInt(20000);
delay (time);
goto piskej;

Whenever alarm(_SignalReceived(1))
{
FRM_MorphSpeech(ge01,0,0,0);
EndScript();
}

label end: