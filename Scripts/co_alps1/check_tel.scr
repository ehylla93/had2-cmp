// phone line

Block {
frame obj;		FRM_FindFrame(obj,"objectives");
frame dab;		FRM_FindFrame(dab,"dabing");
Frame ovrid;	FRM_FindFrame(ovrid,"ovladacridice");

Integer tel = 0;
}

Whenever check (_SignalReceived(1))
{
//Sendsignal(ovrid,2);	//stop talking
Delay(500);
if (tel==0)
{
SendSignal(dab, 1);
}
else { SendSignal(dab, 2); }
}

Whenever disabled (_SignalReceived(2))
{
tel = 1;
goto end;
}

Label end: