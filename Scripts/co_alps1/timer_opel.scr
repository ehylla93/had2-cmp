// timer - start Opel

Block {
Frame starttalk;	Frm_FindFrame (starttalk,"ovladacridice");
Integer rnd=0;
}

goto end;

Whenever go (_SignalReceived(1))
{
goto timer;
}

Label timer:
rnd=_RandomInt(4);
if(rnd==0)
{
Delay(30000);
}
if(rnd==1)
{
Delay(45000);
}
if(rnd==2)
{
Delay(60000);
}
if(rnd==3)
{
Delay(25000);
}
SendSignal(starttalk, 1);


Label end: