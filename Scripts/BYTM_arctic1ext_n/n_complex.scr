// Player near complex (junction), activating E19

Frame complex;	FRM_Findframe(complex, "n_complex_2");
Frame E19;		FRM_Findframe(E19, "n_E19");
frame MyFRM;	FRM_GetMyFrame(MyFRM);

Whenever PIB (_PlayerInBox(MyFRM))
{
SendSignal(complex, 1);
SetWhenever(PIB, false);
printf("Complex(road) activated");
SendSignal(E19, 1);
EndScript();
}

OnSignal(1)
{
SetWhenever(PIB, false);
EndScript();
}