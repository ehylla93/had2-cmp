// Objective 1: "Find the entrance to the underground."

SetObjectiveStatus(1, 0);
Frame obj2;		FRM_FindFrame(obj2,"obj2");

goto end;


Whenever PIR (_PlayerInRange(3))
{
SendSignal(obj2, 1);
SUBTITLES_SetOn(1);
SUBTITLES_SetText(01990205); 	//"OBJECTIVE ACCOMPLISHED: ENTRANCE LOCATED."
Delay(5000);
SUBTITLES_SetOn(0);
SetObjectiveStatus(1, 1);
EndScript();
}

label end: