// Tiger MG gunner

Block
{
  HUMAN_SetOptimized(2);
  SetAlarmType(1023, False); 
}
HUMAN_Suspend(True);	
goto TheEnd;

// posila aktivator treti vlny
Onsignal(1)		// aktivace 
{
  HUMAN_Suspend(False);
  HUMAN_BoardVehicle("Tiger", true, 1);	
  SetAlarmType(1023, True); 
} 

OnAlarm()  
  {
          
    EndScript();
  }
OnAlarmDone()  
{
  HUMAN_SetAlarm(False);
  SetAlarmType(1023, True); 
} 

label TheEnd: