// Tank 4 (La_tiger_4) Ridic
// -------------------------------------

Block
{
Frame Tank;		FRM_FindFrame(Tank,"La_tiger_4");

Integer Jedu;

HUMAN_Suspend(1);
}
goto END;

// -------------------------------------

Whenever Spawn (_SignalReceived(1))
{
HUMAN_Suspend(0);
HUMAN_BoardVehicle("La_tiger_4", 1, 0);
Delay(200000);

Jedu = _RandomInt(2);

If (Jedu == 0)
{
HUMAN_Drive("tank01_02", 30);
HUMAN_Drive("", 0);
}

If (Jedu == 1)
{
HUMAN_Drive("tank01_02", 80);
}

goto END;
}


Label END:
