FRAME me;      		FRM_GetMyFrame(me);

// -------------------------------------------------------------------------------------------------------------

OnDeath(){
  FRM_CreateParticle(53, me);
  PlaySound(6, 2);
  MakeExplosion(me, 5000, 40000);		// odremovat
  EndScript();
}

