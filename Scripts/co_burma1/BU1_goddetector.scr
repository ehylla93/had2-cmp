// qsign05 - BU1_goddetector
Block
{
Frame god;	FRM_FindFrame(god, "Goddess");
Frame q01;	FRM_FindFrame(q01, "qsign01");
Frame q02;	FRM_FindFrame(q02, "qsign02");
Frame q03;	FRM_FindFrame(q03, "qsign03");
Frame q04;	FRM_FindFrame(q04, "qsign04");
Frame q05;	FRM_FindFrame(q05, "qsign05");
Frame q06;	FRM_FindFrame(q06, "qsign06");
