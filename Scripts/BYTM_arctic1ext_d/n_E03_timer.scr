//n_E03_timer
//sniper timer

block
{
Integer time;
frame E03;		FRM_FindFrame(E03, "n_E03");
}

goto end;

//---------------------------------------------------------------------------------------------------

OnSignal(1)
{
printf("Timer E03 started");
}

//---------------------------------------------------------------------------------------------------------

Label timer:
time=((_RandomInt(120000)) + 60000);	// min 1 minutes, max 3 minutes
printfnumber("E03 timer", time);
Delay(time);
SendSignal(E03, 1);

label end: