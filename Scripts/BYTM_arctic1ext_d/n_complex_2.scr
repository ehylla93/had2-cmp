// Player near complex (swamp), activating E19, tank2

Frame tanks;	FRM_Findframe(tanks, "n_tanks");
Frame complex;	FRM_Findframe(complex, "n_complex_1");
Frame E19;		FRM_Findframe(E19, "n_E19");

Whenever PIR (_PlayerInRange(10))
{
SetWhenever(PIR, false);
SendSignal(complex, 1);
Block {
printf("Complex(swamp) activated");
SendSignal(tanks, 1);
SendSignal(E19, 1);
}
EndScript();
}

OnSignal(1)
{
SetWhenever(PIR, false);
EndScript();
}