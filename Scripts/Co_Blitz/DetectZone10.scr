Frame EG40;		FRM_FindFrame(EG40, "EG40");
Frame EG45;		FRM_FindFrame(EG45, "EG45");
}
Whenever Yes(_PlayerInBox(detect))
{
sendsignal (EG25,1);
sendsignal (EH7,1);
sendsignal (EH14,1);
