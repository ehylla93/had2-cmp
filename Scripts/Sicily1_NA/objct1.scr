// Obj 1 - Kill the commander of german garrison
// -----------------------------------------------

Block
{
Frame MSTGAD;		FRM_FindFrame(MSTGAD,"P1_G01");

SetObjectiveStatus(1, 0);
}


Whenever Killed (_Actor_GetState(MSTGAD) == 0)
{
Delay(1000);
SUBTITLES_SetOn(1);
SUBTITLES_SetText(54999611);
Delay(3500);
SetObjectiveStatus(1, 1);
SUBTITLES_SetOn(0);
EndScript();
}
