// Obj 4 - Mine the flaks
// ---------------------------------------------

Block
{
Frame B1;	FRM_FindFrame(B1, "BUM_F1");
Frame B2;	FRM_FindFrame(B2, "BUM_F2");

SetObjectiveStatus(4, 0);
}

Whenever Planted ((_Actor_GetState(B1) == 1) And (_Actor_GetState(B2) == 1))
{	
Delay(1000);
SUBTITLES_SetOn(1);
SUBTITLES_SetText(55999602);
Delay(3500);
SetObjectiveStatus(4, 1);
SUBTITLES_SetOn(0);
EndScript();
}
