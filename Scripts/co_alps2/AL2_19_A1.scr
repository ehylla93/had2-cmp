// AL2_19_A1 - activator for AL2_19

Block {
Frame al2_19;	FRM_FindFrame(al2_19, "AL2_19");
SetWhenever(player, false);
}

Whenever alarm(_SignalReceived(1))	// received from AL2_alarm
{
	SetWhenever(player, true);
	GoTo end;
}

Whenever player(_PlayerInRange(2))
{
	SendSignal(al2_19, 1);
	GoTo end;
}

Label end: