// AL2_09_A1 - activator for AL2_09, AL2_10

Block {
Frame al2_09;	FRM_FindFrame(al2_09, "AL2_09");
Frame al2_10;	FRM_FindFrame(al2_10, "AL2_10");
Frame agent;	FRM_FindFrame(agent, "AL2_Agent");
}

Whenever player((_PlayerInRange(2)) OR (_FrameInRange(agent)<2))
{
	SendSignal(al2_09, 1);
	SendSignal(al2_10, 1);
}