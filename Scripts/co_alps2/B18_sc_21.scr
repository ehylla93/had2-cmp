// 	Flip LightMap Doors

Block {	
Frame al2_08;	FRM_FindFrame(al2_08, "AL2_08");
Frame al2_11;	FRM_FindFrame(al2_11, "AL2_11");

integer Door = 1;
}

OnUse()
  {
	Delay(500);
	SendSignal(al2_08, 1);
	SendSignal(al2_11, 2);
     Delay(100);
     FRM_SetGroupLightMap("B18_sc_21",Door);
     if (Door == 0)
       {
	Door = 1;
        }
    else
       {
	Door = 0;
       }
  }