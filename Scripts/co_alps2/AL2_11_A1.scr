// AL2_11_A1 - activator for AL2_11, AL2_12

Block {
Frame al2_11;	FRM_FindFrame(al2_11, "AL2_11");
Frame al2_12;	FRM_FindFrame(al2_12, "AL2_12");
Frame agent;	FRM_FindFrame(agent, "AL2_Agent");
}

Whenever player((_PlayerInRange(2)) OR (_FrameInRange(agent)<2))
{
	SendSignal(al2_11, 1);
	SendSignal(al2_12, 1);
}