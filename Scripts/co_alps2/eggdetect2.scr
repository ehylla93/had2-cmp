// Easteregg, gold bar detection 2 (secret room with painting/s)

Block {
frame nextd;	FRM_FindFrame(nextd, "eggdetect3");
SetWhenever("jetu", 0);
}

Whenever "jetu"(_ItemInRange(245, 2))
{
SendSignal(nextd, 1);
goto end;
}

Whenever "jetubrzo"(_ItemInRange(245, 2))
{
DisableSignals(1);
goto end;
}

Whenever xxx(_SignalReceived(1))
{
SetWhenever("jetu", 1);
SetWhenever("jetubrzo", 0);
}

SetWheneverTime("jetu", 2000);
SetWheneverTime("jetubrzo", 2000);

label end: