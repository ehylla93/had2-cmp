  integer alarm = 0;


HUMAN_SETAIMODE_Defensive();
HUMAN_SetOptimized(1);
HUMAN_SETMODE_Stand();

SetAlarmType(1024, True);	
SetAlarmType(8, False);	// zvuky OnUse

HUMAN_Suspend(True);				
goto TheEnd;				

OnSignal(13)		// DOPLNIT jeho poslani
{				
  Printf("Hunter 1 GO !!!!!!!!!");		
  HUMAN_Suspend(False);			
}						

label Akce:
 		 FRM_FindFrame(bedna, "H1_box");
