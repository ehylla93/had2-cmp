OnAlarm()
{
  Block
    {
      AIEvent =_GetAlarmType();
      PrintfNumber("Hunter 1: ", AIEvent);
      HUMAN_SetAnim("", 300, 300, False);
      HUMAN_Stop();
      HUMAN_SETMODE_Run();
      HUMAN_SETSIGHTRANGE(150);
      HUMAN_SETHEARRANGE(180);
    }
  HUMAN_DropObject();
  If ( Free_hands == 99 ) 
    { 
 		 FRM_FindFrame(bedna, "H1_box");
		 FRM_FindFrame(Bedna_vizual, "H1_box.m_bed08");   
                   FRM_CreatePhysicalObject(bedna_vizual, 10, false, true);	// ??? tak co - pomocny pokus
	       FRM_SetOn(bedna, False);
 		 FRM_FindFrame(bedna, "");
		 FRM_FindFrame(Bedna_vizual, "");   
    }
  Free_hands = 0; 
  If (Alarm == 1) { EndScript(); }
  Delay(1500);
  HUMAN_Move("Hunter1_fire");
  HUMAN_SetSniper(True, 1);
  HUMAN_SETMODE_Crouch();
  HUMAN_TurnAt(see);
  EndScript();
}

OnAlarmDone()
{
  SetAlarmType(512, False);	  
  SetAlarmType(511, True);	
  HUMAN_SetSniper(False, 1);
  HUMAN_SetAlarm(False);
  HUMAN_SETMODE_Walk();
  HUMAN_SetOptimized(1);
  HUMAN_SETMODE_Stand();
  If (Alarm == 0) 
    {
       HUMAN_Move("Hunter1_2");
       Alarm = 1;
    }
  HUMAN_Move("DeAlarm_1");
  goto TheEnd;
}

OnDeath()
{
  If ( Free_hands == 99 ) 
    { 
 		 FRM_FindFrame(bedna, "H1_box");
		 FRM_FindFrame(Bedna_vizual, "H1_box.m_bed08");   
       FRM_FindFrame(ruka, "Hunter_1.HW1"); 
       FRM_TeleportNearFrame(bedna, ruka, False);
       FRM_FindFrame(ruka, ""); 
       FRM_CreatePhysicalObject(bedna_vizual, 10, false, true);
       FRM_SetOn(bedna, False);
 		 FRM_FindFrame(bedna, "");
		 FRM_FindFrame(Bedna_vizual, "");   
    }
  Endscript();
}

