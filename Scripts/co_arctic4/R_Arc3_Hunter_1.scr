
  integer  AIEvent;

  frame see; 			FRM_FindFrame(see, "dummy_Hunter_1_see");  // DOPLNIT
  frame ruka;			FRM_FindFrame(ruka, ""); 
  frame bedna; 		FRM_FindFrame(bedna, "");
