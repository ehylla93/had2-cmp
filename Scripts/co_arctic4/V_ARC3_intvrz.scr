frame sound1;		frm_findframe(sound1,"S_int_vrz1");
frame sound2;		frm_findframe(sound2,"S_int_vrz2");
frame sound3;		frm_findframe(sound3,"S_int_vrz3");

integer rnd;	
integer rnd_pause;

label loop:
rnd= _randomint(3) ;
rnd_pause= _randomint(16000) +7500;
if (rnd==1) {FRM_SetOn(sound1,true);}
if (rnd==2) {FRM_SetOn(sound2,true);} 
if (rnd==0) {FRM_SetOn(sound3,true);} 
delay(rnd_pause);
goto loop;
